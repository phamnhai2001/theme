{% schema %}
{
  "name": "ranking",
  "tag": "section",
  "class": "spaced-section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "default": "Ranking collection",
      "label": "text-ranking"
    },
    {
      "type": "collection",
      "id": "collection",
      "label": "collection-ranking"
    },
    {
      "type": "range",
      "id": "products_to_show",
      "min": 2,
      "max": 12,
      "step": 2,
      "default": 4,
      "label": "products-to-show"
    },
    {
      "type": "checkbox",
      "id": "show_view_all",
      "default": true,
      "label": "show-view-all"
    },
    {
      "type": "checkbox",
      "id": "swipe_on_mobile",
      "default": false,
      "label": "swip-on-mobile"
    },
    {
      "type": "header",
      "content": "content-header"
    },
    {
      "type": "select",
      "id": "image_ratio",
      "options": [
        {
          "value": "adapt",
          "label": "option-1"
        },
        {
          "value": "portrait",
          "label": "option-2"
        },
        {
          "value": "square",
          "label": "option-3"
        }
      ],
      "default": "adapt",
      "label": "imgae-ratio"
    },
    {
      "type": "checkbox",
      "id": "show_secondary_image",
      "default": false,
      "label": "show-secondary-image"
    },
    {
      "type": "checkbox",
      "id": "add_image_padding",
      "default": false,
      "label": "add-image-padding"
    },
    {
      "type": "checkbox",
      "id": "show_image_outline",
      "default": true,
      "label": "show-image-outline"
    },
    {
      "type": "checkbox",
      "id": "show_vendor",
      "default": false,
      "label": "show-vendor"
    },
    {
      "type": "checkbox",
      "id": "show_rating",
      "default": false,
      "label": "show-rating",
      "info": "info-show-rating"
    }
  ],
  "presets": [
    {
      "name": "presets"
    }
  ]
}
{% endschema %}