<!DOCTYPE html>
<html lang="{{request.locale.iso_code}}">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="canonical" href="{{canonical_url}}">
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    <title>{{page_title}}</title>
    <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>

    {{content_for_header}}
    {{ 'style.css' | asset_url | stylesheet_tag }}
    {{ 'slick.css' | asset_url | stylesheet_tag }}
    {{ 'slick-theme.css' | asset_url | stylesheet_tag }}
    {{ 'select2.min.css' | asset_url | stylesheet_tag }}
    {{ 'xzoom.css' | asset_url | stylesheet_tag }}
    {{ 'owl.carousel.min.css' | asset_url | stylesheet_tag }}
    {{ 'owl.theme.default.min.css' | asset_url | stylesheet_tag }}
    {{ 'jquery.fancybox.min.css' | asset_url | stylesheet_tag }}
  	{{ 'header_icon_search.png' | img_url | stylesheet_tag }}

</head>
<body>
    <div id="container">
        {% section 'header' %}
        <div class="main">
            {%section 'menu'%}
            <div class="inner">
                {%section 'image-banner'%}
                <div id="main_content">
                    {{content_for_layout}}
                </div>
            </div>
        </div>

        {% section 'footer' %}
    </div>
</body>
<script src="{{'jquery.min.js' | asset_url}}"></script>
<script src="{{'slick.min.js' | asset_url}}"></script>
<script src="{{'jquery.fancybox.min.js' | asset_url}}"></script>
<script src="{{'select2.min.js' | asset_url}}"></script>
<script src="{{'jquery.easing.js' | asset_url}}"></script>
<script src="{{'jquery-ui.slider.js' | asset_url}}"></script>
<script src="{{'check_price_search.js' | asset_url}}"></script>
<script src="{{'xzoom.min.js' | asset_url}}"></script>
<script src="{{'jquery.matchHeight-min.js' | asset_url}}"></script>
<script src="{{'owl.carousel.min.js' | asset_url}}"></script>
<script src="{{'main.js' | asset_url}}"></script>


</html>